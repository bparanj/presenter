source 'http://rubygems.org'

gem 'rails', '4.2.4'
gem 'sqlite3'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

gem 'jquery-rails'
gem 'redcarpet'
gem 'rspec-rails', :group => [:development, :test]
